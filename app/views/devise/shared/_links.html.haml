.devise__links
  - if controller_name != 'sessions'
    .block
      = link_to t('.sign_in'), new_session_path(resource_name), class: 'btn-link'
  - if devise_mapping.registerable? && controller_name != 'registrations'
    .block
      = link_to t('.sign_up'), new_registration_path(resource_name), class: 'btn-link'
